<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ToolBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/md_theme_onPrimaryContainer</item>
        <item name="titleTextColor">@color/md_theme_onPrimary</item>
        <item name="subtitleTextColor">@color/md_theme_onPrimary</item>
        <item name="android:padding">10dp</item>
    </style>
    <style name="Button">
        <item name="backgroundTint">@color/md_theme_onPrimaryContainer</item>
        <item name="tint">@color/md_theme_onPrimary</item>
    </style>
</resources>